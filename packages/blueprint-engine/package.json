{
  "name": "@drywall/blueprint-engine",
  "version": "0.0.0",
  "private": true,
  "description": "Canvas-based blueprint drawing engine for drywall estimation",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@drywall/types": "workspace:*",
    "@drywall/core": "workspace:*"
  },
  "devDependencies": {
    "@drywall/tsconfig": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  }
}
